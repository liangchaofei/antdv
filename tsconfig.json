{
	"compilerOptions": {
		"strictNullChecks": true,
		"strict": true,
		"declaration": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"jsx": "react",
		"resolveJsonModule": true,
		"baseUrl": "./",
		"paths": {
			"@@/*": [".dumi/tmp/*"],
			"antdv": ["src"],
			"antdv/*": ["src/*", "*"]
		}
	},
	"include": [".dumirc.ts", "src/**/*"]
}
